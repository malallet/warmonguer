    <constraints><constraint id="f2e1-6a0c-0b38-572c" type="max" value="1" field="selections" scope="roster" /></constraints><profiles><profile id="rp-testarudod46a-b" name="TESTARUDO" typeId="6b2c-3d4e-5f60-8a11"><characteristics><characteristic name="Efecto" typeId="f333-2c3d-4e55-9fb2">Si saca 1 en tirada enfrentada de Combate, puede repetirla.</characteristic></characteristics></profile></profiles></selectionEntry>
